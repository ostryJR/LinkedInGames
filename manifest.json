{
  "manifest_version": 3,
  "name": "LinkedIn Games",
  "version": "0.2",
  "author": "Mikolaj \"Miki\" Ostrowski",
  "description": "Adds Mini Sudoku to LinkedIn Games",
  "content_scripts": [
   {
      "matches": [
        "https://*.linkedin.com/games/",
        "https://*.lnkd.in/games"
      ],
      "js": [
        "gamesPage.js"
      ]
    },
    {
      "matches": [
        "https://*.linkedin.com/feed/",
        "https://*.lnkd.in/feed"
      ],
      "js": [
        "feedPage.js"
      ]
    }
  ],


  "icons": {
    "1024": "linkedinLogo.png"
  }
}
