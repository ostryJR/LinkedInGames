{
  "manifest_version": 2,
  "name": "LinkedIn Games",
  "version": "0.1",
  "author": "Mikolaj \"Miki\" Ostrowski",
  "description": "Adds Tango to LinkedIn Games",
  "permissions": [
    ""
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.linkedin.com/games/",
        "https://*.lnkd.in/games"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "icons": {
    "1024": "linkedinLogo.png"
  }
}
